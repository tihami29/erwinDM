	NOTICE_DT DATE NULL ,
	CLAIM_ROW_ID NUMBER NULL ,
	CLAIM_OFFSET_HASH_TXT VARCHAR NULL ,
	CLAIM_OFFSET_HASH_IND NUMBER NULL ,
