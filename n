
CREATE TABLE CLAIM_FUTURE.BULK_INVOICE
(
	BULK_INVOICE_HASH_TXT VARCHAR NOT NULL ,
	BULK_INVOICE_UNIQUE_ID VARCHAR NULL ,
	ROW_ACTIVATION_EFFECTIVE_TMSTP TIMESTAMP_NTZ NULL ,
	ROW_ACTIVATION_EXPIRATION_TMSTP TIMESTAMP_NTZ NULL ,
	INTERNAL_RAW_ID VARCHAR NULL ,
	BATCH_ID VARCHAR NULL ,
	SOURCE_SYSTEM_CD VARCHAR NULL ,
	SOURCE_SYSTEM_DESC VARCHAR NULL ,
	SOURCE_SYSTEM_TMSTP TIMESTAMP_NTZ NULL ,
	ROW_CREATE_TMSTP TIMESTAMP_NTZ NULL ,
	CHANGE_HASH_TXT VARCHAR NULL ,
	ACTION_CATEGORY_CD VARCHAR NULL ,
	ACTION_CATEGORY_DESC VARCHAR NULL ,
	INVOICE_NBR_CD VARCHAR NULL ,
	STATUS_CATEGORY_CD VARCHAR NULL ,
	STATUS_CATEGORY_DESC VARCHAR NULL ,
	APPROVAL_DT DATE NULL ,
	BULK_INVOICE_TXT VARCHAR NULL ,
	RECEIVED_DT DATE NULL ,
	SPLIT_AMT_EQUALLY_IND NUMBER NULL ,
	BULK_INVOICE_CATEGORY_CD VARCHAR NULL ,
	BULK_INVOICE_CATEGORY_DESC VARCHAR NULL ,
	INVOICE_DT DATE NULL ,
	BULK_INVOICE_ROW_ID NUMBER NULL 
);

