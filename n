	REPORT_TO_AGENT_DT DATE NULL ,
	SPECIAL_INVESTIGATION_IND NUMBER NULL ,
	WIND_HURRICANE_DEDUCTIBLE_AMT NUMBER NULL ,
	WIND_HURRICANE_DEDUCTIBLE_IND NUMBER NULL ,
	LOSS_TYPE_CD VARCHAR NULL ,
	LOSS_TYPE_DESC VARCHAR NULL ,
	SUMMARY_LOSS_CAUSE_CD VARCHAR NULL ,
	SUMMARY_LOSS_CAUSE_DESC VARCHAR NULL ,
	SUMMARY_LOSS_DESC VARCHAR NULL ,
	DETAIL_LOSS_CAUSE_CD VARCHAR NULL ,
