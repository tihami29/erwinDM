
CREATE TABLE [E_1]
( 
	[test]               char(18)  NOT NULL ,
	[demo]               char(18)  NULL 
)
go

CREATE TABLE [E_2]
( 
	[east]               char(18)  NOT NULL ,
	[west]               char(18)  NULL 
)
go

ALTER TABLE [E_1]
ADD CONSTRAINT [XPKE_1] PRIMARY KEY  CLUSTERED ([test] ASC)
go

ALTER TABLE [E_2]
ADD CONSTRAINT [XPKE_2] PRIMARY KEY  CLUSTERED ([east] ASC)
go
