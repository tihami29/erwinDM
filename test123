
CREATE TABLE [E_1]
( 
	[a]                  char(18)  NOT NULL ,
	[z]                  char(18)  NULL 
)
go

ALTER TABLE [E_1]
	ADD CONSTRAINT [XPKE_1] PRIMARY KEY  CLUSTERED ([a] ASC)
go

CREATE TABLE [E_2]
( 
	[test]               char(18)  NOT NULL ,
	[demo]               char(18)  NULL 
)
go

ALTER TABLE [E_2]
	ADD CONSTRAINT [XPKE_2] PRIMARY KEY  CLUSTERED ([test] ASC)
go
